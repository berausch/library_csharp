<h2>Patrons:</h2>
@if (@Model.Count == 0)
{
  <p>No patrons have been added yet!</p>
}
else
  {
    <ul>
      @foreach (var patron in Model)
      {
        <li><a href="/patrons/@patron.GetId()">@patron.GetName()</a></li>
      }
    </ul>
  }

<h4><a href="/patrons/new">Add a new patron</a></h4>
<h4><a href="/">Return home</a></h4>
